{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "^(?!.*\\.)(.*)$",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "$VITE_API_BASE_URL"
  }
}